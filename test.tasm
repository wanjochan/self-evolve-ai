# 简单的TASM测试程序

.section .text
    # 入口点
    entry_point:
        # 设置栈帧
        push rbp
        mov rbp, rsp
        
        # 调用主函数
        call main
        
        # 恢复栈帧并返回
        leave
        ret
    
    # 主函数
    main:
        push rbp
        mov rbp, rsp
        
        # 返回0
        xor rax, rax
        
        leave
        ret 